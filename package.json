{
  "name": "mysite",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "test": "turbo run test",
    "lint:turbo": "turbo run lint",
    "lint:depcruise": "depcruise --config .dependency-cruiser.js apps packages",
