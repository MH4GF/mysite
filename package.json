  "devDependencies": {
    "@biomejs/biome": "1.5.3-nightly.24fcf19",
    "@project/configs": "workspace:^*",
    "dependency-cruiser": "16.2.3",
    "eslint": "8.57.0",
    "lost-pixel": "3.15.1",
    "npm-run-all2": "5.0.2",
    "scaffdog": "3.0.0",
    "turbo": "1.12.4"
  },
  "packageManager": "pnpm@8.15.4"
}
