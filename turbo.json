{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["build"]
    },
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {}
  }
}
